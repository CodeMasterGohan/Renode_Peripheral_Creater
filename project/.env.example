# Renode Peripheral Model Generator Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORG_ID=your_openai_org_id_here_optional

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Milvus Configuration (if not using default localhost)
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_USER=
MILVUS_PASSWORD=

# Local LLM Configuration (optional)
LOCAL_LLM_ENDPOINT=http://localhost:8080
LOCAL_LLM_API_KEY=

# Hugging Face Configuration (for embeddings)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here_optional

# GitHub Configuration (for knowledge base updates)
GITHUB_TOKEN=your_github_token_here_optional
GITHUB_USERNAME=your_github_username_here_optional

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/generator.log

# Development Settings
DEBUG_MODE=false
DRY_RUN=false

# Cache Settings
CACHE_DIR=cache
ENABLE_CACHE=true

# Security Settings
ENCRYPT_KEYS=false
VALIDATE_SSL=true

# Performance Settings
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=120

# Feature Flags
ENABLE_ACCURACY_TRACKING=true
ENABLE_AUTO_UPDATES=false
ENABLE_PROFILING=false

# Integration Settings
ENABLE_GIT_INTEGRATION=false
ENABLE_CI_INTEGRATION=false
WEBHOOK_SECRET=your_webhook_secret_here_optional

# Custom Settings
CUSTOM_TEMPLATE_DIR=
CUSTOM_SCHEMA_DIR=
CUSTOM_KNOWLEDGE_DIR=